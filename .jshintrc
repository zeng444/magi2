{
    "browser": true,//声明浏览器环境
    "boss": true,//霸气外漏的配置项：BOSS允许你在if，for，while里面编写赋值语句
    "curly": true,//使用if和while等结构语句时必须加上{}来明确代码块
    "eqeqeq": true,//使用===或者是!==，而不是==和!=
    "eqnull": true,//允许使用"== null"作比较
    "expr": true,
    "immed": true,//匿名函数调用，使用(function(){ ... }()); 而不是(function(){ ... })();
    "noarg": true,//禁止使用arguments.caller和arguments.callee
    "onevar": true,//函数只允许被var的形式声明一遍
    "quotmark": "double",//true表示字符串可以使用单引号和双引号，但必须保持一致，double表示必须使用双引号
    "smarttabs": true,//
    "trailing": true,//不允许行尾空格
    "undef": true,//所有的非全局变量，在使用前都必须被声明
    "unused": true,//变量声明了但不使用会被警告
    "jquery":true,//暴露全局jQuery和$变量，可以在任何地方随意使用
    "strict": true//必须使用use strict;语法
}